{
  "Type": "${TYPE}",
  "MessageId": "${MESSAGE_ID}",
  "TopicArn": "${TPOIC_ARN}",
  "Message": "${MESSAGE}",
  "Timestamp": "${TIME_STAMP}",
  "SignatureVersion": "${SIGNATURE_VERSION}",
  "Signature": "${SIGNATURE}",
  "SigningCertURL": "${SIGN_IN_CERT_URL}",
  "UnsubscribeURL": "${UNSUBSCRIBE_URL}",
  "MessageAttributes": {
    "SERVICE_NAME": {"Type": "String","Value": "${MESSAGE_ATTRIBUTES_SERVICE_NAME}"},
    "EVENT_TYPE": {"Type": "String","Value": "${MESSAGE_ATTRIBUTES_EVENT_TYPE}"},
    "APPLICATION_ID": {"Type": "String","Value": "${MESSAGE_ATTRIBUTES_APPLICATION_ID}"},
    "PAYLOAD_TYPE": {"Type": "String","Value": "${MESSAGE_ATTRIBUTES_PAYLOAD_TYPE}"},
    "APP_SHORT_NAME": {"Type": "String","Value": "${MESSAGE_ATTRIBUTES_APP_SHORT_NAME}"},
    "TIME_STAMP": {"Type": "String","Value": "${MESSAGE_ATTRIBUTES_TIME_STAMP}"},
    "CLIENT_ATTRIBUTES": {"Type": "String.Array","Value": "${MESSAGE_ATTRIBUTES_CLIENT_ATTRIBUTES}"},
    "ENTITY_TYPE": {"Type": "String","Value": "${MESSAGE_ATTRIBUTES_ENTITY_TYPE}"},
    "ASSET_ID": {"Type": "String","Value": "${MESSAGE_ATTRIBUTES_ASSET_ID}"},
    "ESB_CLIENT_API_VERSION": {"Type": "String","Value": "${MESSAGE_ATTRIBUTES_ESB_CLIENT_API_VERSION}"},
    "BUSINESS_EVENT": {"Type": "String","Value": "${MESSAGE_ATTRIBUTES_BUSINESS_EVENT}"},
    "CLIENT_ROUTING_ATTRIBUTES": {"Type": "String.Array","Value": "${MESSAGE_ATTRIBUTES_CLIENT_ROUTING_ATTRIBUTES}"},
    "MESSAGE_ID": {"Type": "String","Value": "${MESSAGE_ATTRIBUTES_MESSAGE_ID}"},
    "SOURCE": {"Type": "String","Value": "${MESSAGE_ATTRIBUTES_SOURCE}"},
    "PAYLOAD_SIZE": {"Type": "String","Value": "${MESSAGE_ATTRIBUTES_PAYLOAD_SIZE}"},
    "IS_ON_REF_SERVER": {"Type": "String","Value": "${MESSAGE_ATTRIBUTES_IS_ON_REF_SERVER}"},
    "CORRELATION_ID": {"Type": "String","Value": "${MESSAGE_ATTRIBUTES_CORRELATION_ID}"},
    "FILE_NAME": {"Type": "String","Value": "${FILE_NAME}"},
    "EVNT_IND": {"Type": "String","Value": "${EVNT_IND}"}
    
  }
}